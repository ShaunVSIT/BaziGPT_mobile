{
  "expo": {
    "name": "BaziGPT",
    "slug": "BaziGPT_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "owner": "shaunvee",
    "description": "AI-Powered Chinese Astrology & Relationship Compatibility",
    "primaryColor": "#ff9800",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shaunvee.bazigpt",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleDisplayName": "BaziGPT",
        "CFBundleName": "BaziGPT",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff9800"
      },
      "package": "com.shaunvee.bazigpt",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "280e55c6-9262-4fa1-8e5d-c675e1698aa2"
      },
      "apiBaseUrl": "https://www.bazigpt.io/api"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
